{
  "expo": {
    "name": "LifeMe",
    "slug": "LifeMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-lifeme-invert1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-lifeme.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-lifeme-invert1.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
